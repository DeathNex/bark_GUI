<?xml version="1.0" encoding="utf-8"?>
<case xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="XSDValidator.xsd">
  <simulation>
    <start_time unit="s">
      <constant>1</constant>
    </start_time>
    <end_time unit="s">
      <constant>1</constant>
    </end_time>
    <time_step unit="s">
      <constant>1</constant>
    </time_step>
    <space_step unit="m">
      <constant>1</constant>
    </space_step>
    <initial_temperature unit="K">
      <constant>1</constant>
    </initial_temperature>
  </simulation>
  <setup>
    <materials>
      <material name="name1">
        <phase>
          <keyword>solid</keyword>
        </phase>
        <conductivity unit="W/mK">
          <constant>1</constant>
        </conductivity>
        <heat_capacity unit="J/kgK">
          <constant>1</constant>
        </heat_capacity>
        <density unit="kg/m3">
          <constant>1</constant>
        </density>
        <emissivity unit="-">
          <constant>1.0</constant>
        </emissivity>
        <solar_radiation>
          <transmittance unit="-">
            <constant>0.0</constant>
          </transmittance>
          <absorptivity unit="-">
            <constant>1.0</constant>
          </absorptivity>
          <reflectivity unit="-">
            <constant>0.0</constant>
          </reflectivity>
          <reference_thickness unit="m">
            <constant>1</constant>
          </reference_thickness>
        </solar_radiation>
      </material>
    </materials>
    <geometry>
      <height unit="m">
        <constant>1</constant>
      </height>
      <length unit="m">
        <constant>1</constant>
      </length>
      <tilt unit="deg">
        <constant>0.0</constant>
      </tilt>
    </geometry>
    <coordinates>
      <latitude unit="deg">
        <constant>0.0</constant>
      </latitude>
      <longitude unit="deg">
        <constant>0.0</constant>
      </longitude>
      <orientation unit="deg">
        <constant>0.0</constant>
      </orientation>
      <date unit="month">
        <constant>1</constant>
      </date>
      <time unit="hour">
        <constant>0</constant>
      </time>
    </coordinates>
    <layers>
      <layer name="name2">
        <geometry>
          <thickness unit="m">
            <constant>1</constant>
          </thickness>
        </geometry>
        <material reference="name1" />
      </layer>
    </layers>
    <boundaries>
      <boundary name="internal">
        <temperature unit="K">
          <constant>1</constant>
        </temperature>
        <heat_transfer_coefficient unit="W/m2K">
          <constant>1</constant>
        </heat_transfer_coefficient>
        <velocity unit="m/s">
          <constant>0.0</constant>
        </velocity>
        <heat_flux unit="W/m2">
          <constant>0.0</constant>
        </heat_flux>
        <infrared_irradiance unit="W/m2">
          <constant>0.0</constant>
        </infrared_irradiance>
        <solar_irradiance unit="W/m2">
          <constant>0.0</constant>
        </solar_irradiance>
      </boundary>
      <boundary name="external">
        <temperature unit="K">
          <constant>1</constant>
        </temperature>
        <heat_transfer_coefficient unit="W/m2K">
          <constant>1</constant>
        </heat_transfer_coefficient>
        <velocity unit="m/s">
          <constant>1</constant>
        </velocity>
        <heat_flux unit="W/m2">
          <constant>1</constant>
        </heat_flux>
        <infrared_irradiance unit="W/m2">
          <constant>1</constant>
        </infrared_irradiance>
        <solar_irradiance unit="W/m2">
          <constant>1</constant>
        </solar_irradiance>
      </boundary>
    </boundaries>
  </setup>
  <output>
    <screen>
      <time_step unit="s">
        <constant>1</constant>
      </time_step>
    </screen>
    <file>
      <time_step unit="s">
        <constant>1</constant>
      </time_step>
      <space_step unit="m">
        <constant>1</constant>
      </space_step>
    </file>
  </output>
</case>