<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="XSDValidatorUnits.xsd"/>
  <xs:include schemaLocation="XSDValidatorComplexTypes.xsd"/>
  <xs:include schemaLocation="XSDValidatorFunctions.xsd"/>
  <xs:include schemaLocation="XSDValidatorUtility.xsd"/>
  

  <!--SEARCH-TAGS:
  FIXHERE
  CHECKHERE
  
  (END) StartElement <name>
  (END) BaseElement <name>
  (END) InnerElement D(#) <name>
  (END) AttributeElement <name>
  Define <name>:
  -->




  <!--StartElement <case>-->
  <xs:element name="case">
    <xs:complexType>
      <xs:sequence>
        <!--BaseElement <simulation>-->
        <xs:element name="simulation">
          <xs:complexType>
            <xs:all>
              <xs:annotation>
                <xs:documentation>This is where the help text can exist.</xs:documentation>
              </xs:annotation>

              <xs:element name="start_time" type="time">
                <xs:annotation>
                  <xs:documentation>This is also where the help text can exist.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="end_time" type="time"/>
              <xs:element name="time_step" type="time_positive"/>
              <xs:element name="space_step" type="space"/>
              <xs:element name="initial_temperature" type="initial_temperature"/>

            </xs:all>
          </xs:complexType>
        </xs:element>
        <!--END BaseElement <simulation>-->




        <!--BaseElement <setup>-->
        <xs:element name="setup">
          <xs:complexType>
            <xs:sequence>
              <!--InnerElement D1 <materials>-->
              <xs:element name="materials">
                <xs:complexType>
                  <xs:sequence>
                    <!--InnerElement D2 <material>-->
                    <xs:element name="material" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:all>

                          <xs:element name="conductivity" type="conductivity" />
                          <xs:element name="heat_capacity" type="heat_capacity" />
                          <xs:element name="density" type="density" />
                          <xs:element name="emissivity" type="emissivity" minOccurs="0" />
                          
                          <!--InnerElement D3 <solar_radiation>-->
                          <xs:element name="solar_radiation" minOccurs="0">
                            <xs:complexType>
                              <xs:all>
                                <xs:element name="transmittance" type="transmittance" minOccurs="0" />
                                <xs:element name="absorptivity" type="absorptivity" />                  <!-- CHECKHERE if required -->
                                <xs:element name="reflectivity" type="reflectivity" />                   <!-- CHECKHERE if required -->
                                <xs:element name="reference_thickness" type="space" minOccurs="0" />      <!-- CHECKHERE if new type is needed -->
                              </xs:all>
                            </xs:complexType>
                          </xs:element>
                          <!--END InnerElement D3 <solar_radiation>-->

                        </xs:all>
                        <xs:attribute name="name" type="xs:string" use="required" />
                        <xs:attribute name="phase" type="material_phase" default="solid" />

                      </xs:complexType>
                    </xs:element>
                    <!--END InnerElement D2 <material>-->
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <materials>-->

              <!--InnerElement D1 <geometry>-->
              <xs:element name="geometry">
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="height" type="space" />
                    <xs:element name="length" type="space" />
                    <xs:element name="tilt" type="angle" />

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <geometry>-->

              <!--InnerElement D1 <coordinates>-->
              <xs:element name="coordinates" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="latitude" type="angle" minOccurs="0" />
                    <xs:element name="longitude" type="angle" minOccurs="0" />
                    <xs:element name="orientation" type="angle" minOccurs="0" />
                    <xs:element name="date" type="dateMonth" minOccurs="0" />
                    <xs:element name="time" type="dateTime" minOccurs="0" />

                  </xs:sequence>
                  <xs:attribute name="name" type="xs:string" default="Unknown" />

                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <coordinates>-->

              <!--InnerElement D1 <layers>-->
              <xs:element name="layers">
                <xs:complexType>
                  <xs:choice>

                    <!--InnerElement D2 <layer>-->
                    <xs:element name="layer" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <!--InnerElement D3 <geometry>-->
                          <xs:element name="geometry">
                            <xs:complexType>
                              <xs:sequence>

                                <xs:element name="thickness" type="space" />                                <!-- CHECKHERE type -->

                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <!--END InnerElement D3 <geometry>-->

                          <!--AttributeElement <material>-->
                          <xs:element name="material" type="reference"/>
                        </xs:sequence>
                        <xs:attribute name="name" type="xs:string" default="Unknown" />

                      </xs:complexType>
                    </xs:element>
                    <!--END InnerElement D2 <layer>-->

                  </xs:choice>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <layers>-->

              <!--InnerElement D1 <boundaries>-->
              <xs:element name="boundaries">
                <xs:complexType>
                  <xs:choice>

                    <!--InnerElement D2 <boundary>-->
                    <xs:element name="boundary" minOccurs="2" maxOccurs="2">
                      <xs:complexType>
                        <xs:sequence>

                          <xs:element name="temperature" type="temperature" />
                          <xs:element name="heat_transfer_coefficient" type="heat_transfer_coefficient" />
                          <xs:element name="velocity" type="velocity" />
                          <xs:element name="heat_flux" type="heat_flux" />
                          <xs:element name="infrared_irradiance" type="heat_flux" minOccurs="0" />         <!-- CHECKHERE if new type is needed -->
                          <xs:element name="solar_irradiance" type="heat_flux" minOccurs="0" />         <!-- CHECKHERE if new type is needed -->

                        </xs:sequence>
                        <xs:attribute name="name" type="xs:string" default="Unknown" />

                      </xs:complexType>
                    </xs:element>
                    <!--END InnerElement D2 <boundary>-->

                  </xs:choice>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <boundaries>-->

            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--END BaseElement <setup>-->





        <!--BaseElement <output>-->
        <xs:element name="output">
          <xs:complexType>
            <xs:sequence>

              <!--InnerElement D1 <screen>-->
              <xs:element name="screen">
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="time_step" type="time_positive"/>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <screen>-->

              <!--InnerElement D1 <file>-->
              <xs:element name="file">
                <xs:complexType>
                  <xs:sequence>

                    <xs:element name="time_step" type="time_positive"/>
                    <xs:element name="space_step" type="space"/>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <file>-->

              <!--InnerElement D1 <graph>-->
              <!-- FIXHERE -->
              <xs:element name="graph" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>

                    <!--AttributeElement <filename>-->
                    <xs:element name="filename">
                      <xs:complexType>
                        <xs:attribute name="name" type="xs:string"/>
                        <xs:attribute name="filename" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                    <!--AttributeElement <xvariable>-->
                    <xs:element name="xvariable">
                      <xs:complexType>
                        <xs:attribute name="title" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>
                    <!--AttributeElement <yvariable>-->
                    <xs:element name="yvariable">
                      <xs:complexType>
                        <xs:attribute name="title" type="xs:string"/>
                      </xs:complexType>
                    </xs:element>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <!--END InnerElement D1 <graph>-->
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--END BaseElement <output>-->



      </xs:sequence>
    </xs:complexType>


    
    
    <!-- KEYS -->
    <xs:key name="material_key">
      <xs:selector xpath="setup/materials/material"/>
      <xs:field xpath="@name"/>
    </xs:key>
    <xs:keyref name="material_key_ref" refer="material_key">
      <xs:selector xpath="setup/layers/layer/material"/>
      <xs:field xpath="@reference"/>
    </xs:keyref>
    
    
    
    
    
    
  </xs:element>
  <!--END StartElement <case>-->


</xs:schema>